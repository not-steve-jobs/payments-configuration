{
  "name": "payments-configuration-management-service",
  "version": "0.0.271",
  "private": true,
  "license": "ISC",
  "main": "build/server.js",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.19.2",
    "yarn": "Please use NPM!"
  },
  "scripts": {
    "watch": "tsc --watch",
    "start": "node build/server.js",
    "start:dev": "ENABLE_CONSOLE_LOGS=1 ts-node-dev --inspect=0.0.0.0:9229 --transpile-only -r tsconfig-paths/register --project ./tsconfig.json --files src/server.ts",
    "start:test": "ENABLE_CONSOLE_LOGS=1 nyc --reporter=html --reporter=text --report-dir=test/component/results/coverage node --inspect=0.0.0.0:9229 build/server.js --config=component-test.properties | tee test/component/results/service.log",
    "start:prod": "if [ \"$STARTUP_MODE\" = \"migration\" ]; then npm run migration:up:prod; else npm run start; fi",
    "migration:up:prod": "typeorm-ts-node-commonjs -d build/db/database-config.js migration:run",
    "test": "npm run test:unit && npm run test:component:report",
    "test:unit": "jest --runInBand --ci --logHeapUsage --no-cache --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clear-cache": "jest --clearCache",
    "test:setup-test-db-deprecated": "ENV_PROPERTIES=component-test.properties db-migrations-recreate-and-seed",
    "test:setup-test-db": "export ENV_PROPERTIES=component-test.properties && npm run typeorm:drop && npm run typeorm:create && npm run migration:up && npm run seed:run",
    "test:component": "ENV_PROPERTIES=component-test.properties jest --runInBand --config test/component/jest.config.ts",
    "test:regress": "ENV_PROPERTIES=component-test.properties jest --runInBand --config test/regress/jest.config.ts",
    "test:component:report": "ENV_PROPERTIES=component-test.properties nyc --nycrc-path=node_modules/@internal/component-test-library/lib/cfg/.nycrc jest --ci --runInBand --config test/component/jest.config.ts",
    "build": "npm install && npm run build:api && tsc --build ./tsconfig.build.json && tsc-alias",
    "build:api": "typegen openapi.yaml > ./typings/openapi.d.ts",
    "clean": "rm -rf node_modules build coverage",
    "clean:build": "rm -rf build coverage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "lint:tests": "tsc --project ./tsconfig.jest.json --noEmit && npm run lint -- --fix",
    "ci:code-style": "npm run lint",
    "ci:test": "tsc --build ./tsconfig.jest.json && npm run test:setup-test-db-deprecated && npm run test",
    "ci:build": "tsc --build ./tsconfig.build.json && tsc-alias",
    "typeorm": "typeorm-ts-node-commonjs -d src/db/database-config.ts",
    "typeorm:ex": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs",
    "typeorm:create": "npm run typeorm:ex -- db:create -d src/db/database-config.ts",
    "typeorm:drop": "npm run typeorm:ex -- db:drop  -d src/db/database-config.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/db/migrations/$npm_config_name",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/db/migrations/$npm_config_name",
    "migration:up": "npm run typeorm -- migration:run",
    "migration:down": "npm run typeorm -- migration:revert",
    "seed:run": "npm run typeorm:ex -- seed:run -d src/db/database-config.ts"
  },
  "dependencies": {
    "@internal/cache-library": "3.0.5",
    "@internal/config-library": "1.0.3",
    "@internal/core-library": "2.4.18",
    "@internal/errors-library": "1.0.101",
    "@internal/logger-library": "3.0.12",
    "@internal/redis-client": "1.0.24",
    "@internal/request-library": "3.1.4",
    "@internal/telemetry-library": "1.0.7",
    "awilix": "8.0.0",
    "knex": "3.1.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.4.0",
    "ioredis": "5.4.1",
    "cors": "2.8.5",
    "passport": "0.7.0",
    "passport-azure-ad": "4.3.5"
  },
  "devDependencies": {
    "@internal/chassis-cli": "0.0.39",
    "@internal/component-test-library": "1.0.47",
    "@internal/db-migrations": "1.2.172",
    "@internal/eslint-config-mw-development": "1.0.5",
    "@internal/jest-extends": "0.0.4",
    "@types/jest": "29.5.3",
    "@types/node": "^16.0.0",
    "@types/passport": "1.0.16",
    "@types/passport-azure-ad": "4.3.6",
    "casual": "^1.6.2",
    "jest": "29.6.2",
    "jest-extended": "^3.2.4",
    "nyc": "^15.1.0",
    "openapi-client-axios-typegen": "^5.3.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.5",
    "@types/cors": "2.8.17"
  },
  "author": "",
  "keywords": []
}
